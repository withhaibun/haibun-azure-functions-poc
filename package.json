{
  "name": "haibun-azure-functions-mqtt-poc",
  "type": "module",
  "version": "1.29.2",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start --verbose",
    "stack": "concurrently --kill-others \"npm run start-azureite\" \"npm run start-testserver\" \"func start --verbose\"",
    "test": "echo \"No tests yet...\"",
    "start-azureite": "azurite --location /tmp/azurite --debug /tmp/azurite/debug.log",
    "start-testserver": "http-server test/ --port 9023",
    "start-silent-azureite": "azurite-blob --silent --location /tmp/azurite --debug /tmp/azurite/debug.log --blobHost",
    "zip": "zip -FSr deploy.zip dist"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0-alpha.7",
    "durable-functions": "^3.0.0-alpha.5",
    "http-server": "^14.1.1",
    "playwright": "^1.37.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "azure-functions-core-tools": "^4.0.5198",
    "azurite": "^3.25.1",
    "concurrently": "^8.2.0",
    "typescript": "^5.1.3"
  }
}
